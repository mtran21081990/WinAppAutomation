<Page x:Class="SampleWpfApplication.Views.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SampleWpfApplication.Views"
      mc:Ignorable="d" 
      d:DesignHeight="200" d:DesignWidth="300" Title="Page1">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" >
            <TextBlock FontWeight="Bold">Textbox in Frame</TextBlock>
        </StackPanel>
        <StackPanel Grid.Row="1">
            <TextBox  AutomationProperties.AutomationId="textbox-frame-1" Name="TextboxFrame1" />
        </StackPanel>
    </Grid>
</Page>
